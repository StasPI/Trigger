    "Password": "Password1",
    "HostName": "host.docker.internal",
    "Port": 5800,
    "VirtualHost": "/",
    "ProducerEvent": {
      "ExchangeName": "ExchangeEvent",
      "RoutingKeyName": "KeyEvent"
