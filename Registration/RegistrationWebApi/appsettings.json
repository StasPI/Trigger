{
  "ConnectionStrings": {
    "Postgres": "Host=host.docker.internal;Port=5432;Database=TriggerRegistration;Username=StasPI;Password=Password1"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
